{
  "$schema": "https://swc.rs/schema.json",
  "sourceMaps": true,
  "outputPath": "./esm",
  "jsc": {
    "baseUrl": "./",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    }
  },
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": false,
    "noInterop": false,
    "lazy": false
  },
  "minify": true
}
