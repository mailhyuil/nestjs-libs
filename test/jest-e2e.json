{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@mailhyuil/nestjs-libs/nestjs-libs": "<rootDir>/../libs/nestjs-libs/src",
    "@mailhyuil/nestjs-libs/nestjs-libs/(.*)": "<rootDir>/../libs/nestjs-libs/src/$1"
  }
}
